reporte.autorizarFacturas = SELECT f.CODIGO,f.NUMERO FROM V_INFO_FACTURA_MAESTRO f where f.CODIGO||f.NUMERO not in (SELECT e.codigo||e.numero FROM ELE_DOCUMENTO_ELECTRONICO e where e.ESTADO='AUTORIZADO') and f.FECHA like ?
reporte.guiasRemisionAutorizadas = SELECT f.CODIGO,f.NUMERO,f.FECHA,f.PLACA,f.DOCUMENTO,f.RAZON_SOCIAL_TRANSPORTISTA,f.DIRECCION_PARTIDA,FUN_CLAVE_ACCESO(f.FECHA,f.CODIGO,f.NUMERO) as xml,FUN_CLAVE_ACCESO(f.FECHA,f.CODIGO,f.NUMERO) as pdf FROM ELE_DOCUMENTO_ELECTRONICO e INNER JOIN V_INFO_GUIA_REMISION f ON f.CODIGO  = e.CODIGO AND f.NUMERO = e.NUMERO where e.ESTADO='AUTORIZADO' and FECHA like ?
 
